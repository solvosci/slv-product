<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="product_template_form_view" model="ir.ui.view">
        <field name="name">Product template Secondary Unit (in product_secondary_unit_factor_digits)</field>
        <field name="model">product.template</field>
        <field
            name="inherit_id"
            ref="product_secondary_unit.product_template_form_view"
        />    
        <field name="priority" eval="999" />
        <field name="arch" type="xml">
            <field name="factor" position="attributes">
                <attribute name="digits">[12,6]</attribute>
            </field>
        </field>
    </record>
</odoo>
